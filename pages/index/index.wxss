/**index.wxss**/

.all .header {
  box-sizing: border-box;
  position: relative;
  width: 100%;
}

.all .header image {
  width: 100%;
  max-height: 423rpx;
}

.all .header .headtitle {
  position: absolute;
  top: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 45rpx;
  color: #fff;
}

.all .header .headtxet {
  position: absolute;
  top: 160rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 26rpx;
  color: #fff;
}

.all .con {
  width: 100%;
}

.all .con .con_box1 {
  width: 100%;
  padding: 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.all .con .con_box1 view {
  width: 32%;
  height: 220rpx;
  border-radius: 20rpx;
  text-align: center;
}

.con .con_box1 .hy {
  background-color: #ff6b4e;
}

.con .con_box1 .zp {
  background-color: #3187ec;
}

.con .con_box1 .zx {
  background-color: #ffc953;
}

.con .con_box1 view image {
  max-width: 70rpx;
  margin-top: 40rpx;
}

.con .con_box1 view text {
  display: block;
  margin-top: 10rpx;
  color: #fff;
}

.all .con .con_box2 {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx;
  box-sizing: border-box
}

.all .con .con_box2 view {
  width: 49%;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 20rpx;
  text-align: center;
}

.con .con_box2 view image {
  width: 60rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.con .con_box2 view text {
  display: inline-block;
  color: #fff;
}

.con .con_box2 .dc {
  background-color: #adda4c;
}

.con .con_box2 .zs {
  background-color: #b2ca3b;
}

.all .con .con_box3 {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx;
  box-sizing: border-box
}

.all .con .con_box3 view {
  width: 49%;
  border-radius: 20rpx;
  text-align: center;
  padding: 50rpx;
  box-sizing: border-box
}

.con .con_box3 view image {
  max-width: 80rpx;
  height: 90rpx;
}

.con .con_box3 view text {
  display: block;
  margin-top: 10rpx;
  color: #fff;
}

.con .con_box3 .fb {
  background-color: #f4334a;
}

.con .con_box3 .sc {
  background-color: #05a9e8;
}
.limit{
  width: 80rpx!important;
  margin: 0 auto;
  text-align: center;
  height: 128rpx!important;
  padding: 0!important
}

.reward{
  position: fixed;
  bottom: 10rpx;
  transform: translateY(-50%);
  right: 10rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  z-index: 999;
  cursor: pointer
}

.reward image{
  width: 100%;
  border-radius: 50%;
  position: absolute;
  left: 0;
}

.rewardimg{
  background: rgba(0,0,0,0.7);
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9
}
.rewardimg .limit{
  width: 400rpx!important;
  height: 800rpx!important;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%)
}
.rewardimg .limit .imgs{
  width: 400rpx;
  height: 400rpx;
  background: #fff;
}

.rewardimg .limit text{
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  padding: 10rpx 0;
  display: inline-block 
}

.rewardimg .limit .close{
  width: 70rpx;
  height: 70rpx;
  display: block;
  margin: 20rpx auto
}

.reward text{
  font-size: 36rpx;
  color: #05a9e8
}

.rewardimg text{
  font-size: 30rpx;
  padding: 10rpx 0
}
.rewardimg view{
  color: #fff;
  font-size: 36rpx
}

.link text{
  font-size: 24rpx
}

.rewardimg .title{
  font-size: 36rpx !important
}

.tag{
    width:30%;
    padding: 32rpx 0;
    position:fixed;
    background-color:transparent;
    z-index: 999;
    color: #fff;
    right: 0;
    font-size: 26rpx
}

.tag image{
  position: absolute;
  left: 0;
  top: 0;
  width: 40rpx
}

.tag image.close{
  width: 20rpx;
  position: relative
}