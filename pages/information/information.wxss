/* pages/information/information.wxss */
/* pages/meeting/meeting.wxss */

.header {
  box-sizing: border-box;
  position: relative;
  width: 100%;
}

.header image {
  width: 100%;
  max-height: 200rpx;
}

.header .search {
  position: absolute;
  top: 50%;
  width: 90%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.header .search input {
  height: 80rpx;
  background-color: #fff;
  border-radius: 40rpx;
  font-size: 30rpx;
  color: #999;
  line-height: 80rpx;
  padding-left: 30rpx;
}

.header .search image.searchimg {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 6%;
  top: 15rpx;
  z-index: 666666;
  cursor: pointer
}

.header .search text {
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.meet_con {
  width: 100%;
}

.meet_con .meet_title {
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.meet_con .meet_title text {
  height: 90rpx;
  line-height: 90rpx;
  color: #999;
  font-size: 30rpx;
  width: 50%;
  text-align: center;
  border-bottom: 4rpx solid transparent;
}

.meet_con .meet_title text.active {
  border-bottom: 4rpx solid #9bc1f3;
  color: #9bc1f3;
}

.meet_con .meet_box {
  height: auto;
  margin: 0 20rpx;
  background: #fff;
  padding: 22rpx;
  margin-bottom: 20rpx;
}

.meet_con .meet_box ul li {
  line-height: 40rpx;
  display: block;
  color: #999;
  font-size: 24rpx;
}

.meet_con .meet_box ul li image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}

.meet_con .meet_box ul .meet_tt {
  color: #333;
  font-size: 30rpx;
}

.all {
  height: 100vh;
  background: #f4f4f4;
  position: absolute;
  bottom: 0;
  top: 0;
  width: 100%;
  left: 0;
}

.meet_title picker {
  font-size: 26rpx;
  padding: 30rpx 0;
  text-align: center;
  width: 33%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.meet_title picker icon {
  width: 26rpx;
  height: 14rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100%;
}

/* .meet_title picker.active icon {
  width: 26rpx;
  height: 12rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100%;
} */

.meet_title picker view {
  display: flex;
  align-items: center;
  justify-content: center;
}

.meet_title picker icon {
  margin-left: 10rpx;
}

.zhaopin .meet_tt image {
  width: 80rpx !important;
  height: 80rpx !important;
}

.zhaopin .meet_tt {
  display: flex !important;
  align-items: center;
  justify-content: space-between;
}

.zhaopin .meet_tt .xueli {
  flex: 1;
  margin-left: 15rpx;
}

.zhaopin .meet_tt {
  font-size: 30rpx;
}

.zhaopin .meet_tt .money {
  font-size: 24rpx;
  color: #61a4ff;
}

.zhaopin .zhiwei {
  font-size: 24rpx;
  color: #333 !important;
  line-height: inherit !important;
  padding: 30rpx 0 20rpx 0;
}

ul {
  overflow: hidden;
  display: block;
}

.zhaopin  .yaoqiu {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.zhaopin  .yaoqiu text {
  padding: 0 20rpx;
  position: relative;
}

.zhaopin  .yaoqiu text::after {
  position: absolute;
  content: '';
  right: 0;
  height: 100%;
  width: 2rpx;
  background: #999;
  top: 0;
}

.zhaopin  .yaoqiu text:last-child::after {
  display: none;
}


ul.lists{
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx
}

ul.lists li{
  width: 45%;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  font-size: 24rpx;
  color: #333;
  text-align: center;
  margin: 17rpx
}

ul.lists li .limit{
  overflow: hidden;
  position: relative;
  height: 0;
  padding: 35% 0;
  text-align: center
}

ul.lists li .limit image{
  position: absolute;
  max-width: 70%;
  overflow: hidden;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}

ul.lists li text{
  padding: 22rpx;
  display: block
}

.shadow{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #f4f4f4;
  z-index: 1
}
.main{
  position: absolute;
  width: 100%;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 99
}